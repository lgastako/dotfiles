# Really this should just be set when it's Linux or rather when operating under the gnome desktop
# environment specifically but for now removing it here is fine... mostly because I'm on a plane
# and I want to commit this and I don't have a Linux box here :)
unalias open

alias m=mate

source /usr/local/bin/virtualenvwrapper.sh
export PATH=/opt/local/bin:/opt/local/sbin:$PATH

# TODO - only if not already set?
export JAVA_HOME=`/usr/libexec/java_home`

